{
  "name": "llamasearch-extension",
  "private": true,
  "version": "0.0.1.2",
  "type": "module",
  "description": "Always go to the right domains and avoid scam pages",
  "displayName": "LlamaSearch",
  "scripts": {
    "search": "vite",
    "test": "jest",
    "css": "postcss ./src/global.css -o ./src/output.css",
    "css:watch": "postcss ./src/global.css -o ./src/output.css --watch",
    "dev": "rm -f vite.config.ts.*.mjs && concurrently \"yarn css:watch\" \"nodemon\"",
    "dev:firefox": "rm -f vite.config.ts.*.mjs && BROWSER=FIREFOX concurrently \"yarn css:watch\" \"nodemon\"",
    "build": "tsc --noEmit && yarn css && vite build",
    "zip": "web-ext build -s dist -a packed -n extension.zip -o",
    "prep": "rm -f vite.config.ts.*.mjs && rm -rf dist packed && yarn build && yarn zip"
  },
  "dependencies": {
    "@headlessui-float/react": "^0.13.2",
    "@headlessui/react": "^1.7.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.197",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.3.1",
    "ts-jest": "^29.1.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "web-ext": "^7.2.0"
  }
}
